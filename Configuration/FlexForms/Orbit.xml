<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<ROOT>
		<type>array</type>
		<el>
			<settings.fileCollections>
				<TCEforms>
					<label>
						Files
					</label>
					<config>
						<type>group</type>
						<internal_type>db</internal_type>
						<size>4</size>
						<maxitems>1000</maxitems>
						<minitems>0</minitems>
						<allowed>sys_file_collection</allowed>
					</config>
				</TCEforms>
			</settings.fileCollections>
			<settings.allowDownload>
				<TCEforms>
					<label>
						Alle Download
					</label>
					<config>
						<type>check</type>
						<default>0</default>
					</config>
				</TCEforms>
			</settings.allowDownload>
			<settings.enableLightbox>
				<TCEforms>
					<label>
						Details in Lightbox
					</label>
					<config>
						<type>check</type>
						<default>0</default>
					</config>
				</TCEforms>
			</settings.enableLightbox>
			<settings.gallery.itemsPerPage>
				<TCEforms>
					<label>
						Limit
					</label>
					<config>
						<type>input</type>
						<eval>int</eval>
						<range>
							<min>1</min>
							<max>200</max>
						</range>
						<default>16</default>
					</config>
				</TCEforms>
			</settings.gallery.itemsPerPage>
		</el>
	</ROOT>
</T3DataStructure>